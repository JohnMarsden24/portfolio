<h1><%= @project.name %></h1>
<h4>links</h4>
<%= link_to @project.url, @project.url %>
<%= link_to @project.github, @project.github %>
<h4>Technology used</h4>
<ul>
  <% @project.technologies.each do |technology| %>
    <li>
      <%= technology %>
    </li>
  <% end %>
</ul>
<h4>Project description</h4>
<p>
  <%= @project.description %>
</p>
<h4>Project difficulties</h4>
<p>
  <%= @project.difficulties %>
</p>
<h4>Project solution</h4>
<p>
  <%= @project.solution %>
</p>
<h4>Project features</h4>
<ul>
  <% @project.features.each do |feature| %>
    <li>
      <%= feature %>
    </li>
  <% end %>
</ul>
<h4>Project photos</h4>
<% @project.photos.each do |photo| %>
  <%= cl_image_tag photo.key, height: 300, width: 400, crop: :fill %>
<% end %>
