<%= link_to "<i class='fas fa-long-arrow-alt-left mtb2'></i> Back".html_safe, admin_home_path, class: "no-d"  %>
<%= simple_form_for @project do |f| %>
  <%= f.input :name %>
  <%= f.input :color %>
  <%= f.input :url %>
  <%= f.input :github %>
  <%= f.input :technologies %>
  <%= f.input :description %>
  <%= f.input :difficulties %>
  <%= f.input :solution %>
  <%= f.input :features %>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <%= f.button :submit, class: "btn-light" %>
<% end %>
