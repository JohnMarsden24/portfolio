<div id="carouselExampleFade" class="carousel slide carousel-fade center" data-ride="carousel" data-slide="true">
  <ol class="carousel-indicators">
    <% @project.photos[1..-1].each_with_index do |photo, index| %>
      <li data-target="#carouselExampleFade" data-slide-to="<%= index %>" class="<%= @project.get_first_item(index) %>"></li>
    <% end %>
  </ol>
  <div class="carousel-inner" data-touch="true">
    <% @project.photos[1..-1].each_with_index do |photo, index| %>
      <div class="carousel-item d-flex justify-content-center <%= @project.get_first_item(index) %>">
        <div class="carousel-slide-photo" style="background-image: url('<%= cl_image_path photo.key %>')">
        </div>
      </div>
    <% end %>
  </div>
</div>
