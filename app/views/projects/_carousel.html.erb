<div id="carouselExampleFade" class="carousel slide carousel-fade center" data-ride="carousel">
  <ol class="carousel-indicators">
    <% @project.photos.each_with_index do |photo, index| %>
      <li data-target="#carouselExampleFade" data-slide-to="<%= index %>" class="<%= @project.get_first_item(index) %>">HEY</li>
    <% end %>
  </ol>
  <div class="carousel-inner pb2">
    <% @project.photos.each_with_index do |photo, index| %>
      <div class="carousel-item d-flex justify-content-center <%= @project.get_first_item(index) %>" >
        <div class="carousel-slide-photo my-shadow" style="background-image: url('<%= cl_image_path photo.key %>')">
        </div>
      </div>
    <% end %>
  </div>
</div>
